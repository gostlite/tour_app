{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "projectSettings": {
    "framework": null,
    "buildCommand": "npm run build:js",
    "outputDirectory": "public",
    "installCommand": "npm install"
  }
}
